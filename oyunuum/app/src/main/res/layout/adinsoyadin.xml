<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TableLayout
        android:id="@+id/TableLayout25"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TableRow
                android:id="@+id/tableRowAna1"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>

            <TableRow
                android:id="@+id/tableRowAna2"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>
            <TableRow
                android:id="@+id/tableRowAna3"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>
            <TableRow
                android:id="@+id/tableRowAna4"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>

            <TableRow
                android:id="@+id/tableRowAna5"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <TextView
                    android:text="boşluk"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView4"
                    android:cursorVisible="false"
                    android:visibility="invisible" />

                <TextView
                    android:text="Adınız:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ad" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/editText" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRowAna6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1" >

                <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView5"
                    android:visibility="invisible" />

                <TextView
                    android:text="Soyadınız:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/soyad" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/editText2" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRowAna7"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>
            <TableRow
                android:id="@+id/tableRowAna8"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>
            <TableRow
                android:id="@+id/tableRowAna9"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

                <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView9"
                    android:visibility="invisible" />

                <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView8"
                    android:visibility="invisible" />

                <Button
                    android:text="Kaydet"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/kaydet" />
            </TableRow>
            <TableRow
                android:id="@+id/tableRowAna10"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

            </TableRow>

        </TableLayout>

    </TableLayout>

</LinearLayout>